    <script>
        $(window).scroll(function () {
            if ($(this).scrollTop() > 50) {

                $('.navbar').css('background', '#263238');

            } else {
                $('.navbar').css('background', '#263238');
            }
        });

        window.onload = function () {
            $(".loading").hide();
        }


        function Ara(fileNAme) {
            $(".loading").show();
            if (fileNAme != "") {
                var datar = { "FileName": fileNAme }

                $("#runningg").addClass("running");
                $.ajax({
                    url: "@Url.Action("ConverttoMp3", "Home")",
                    contentType: "application/json; charset=utf-8",
                    type: "POST",
                    data: JSON.stringify(datar),
                    dataType: "json",
                    success: function (response) {
                        var videoUrl = document.getElementById("yaz").value;
                        var videoKod = "a";
                        for (var i = 0; i < videoUrl.length; i++) {
                            if (videoUrl[i] == '=') {
                                videoKod = videoUrl.substring(i + 1, i + 12);
                                break;
                            }
                        }

                        if (response["snc"] == true) {
                            //  alert("Conversion Succeeded.You Can Download Video");
                            $(".loading").hide();
                            $.confirm({
                                title: 'YouTube to mp3',
                                boxWidth: '100%',
                                content: '<h4>' + response["title"] + '</h1>' +
                                    '<iframe width="100%" height="240" src="https://www.youtube.com/embed/' +
                                    videoKod +
                                    '"' +
                                    ' frameborder="0" allow="accelerometer; autoplay; encrypted-media;' +
                                    ' gyroscope; picture-in-picture" allowfullscreen></iframe>',
                                theme: 'supervan',
                                buttons: {
                                    cancel: {
                                        text: '<div style="width:100%;height:100%;padding:15px">İptal Et</div>',
                                        btnClass: 'btn-red'
                                    },
                                    specialKey: {
                                        text: '<div style="background:#27AE60;width:100%;height:100%;padding:15px"><i class="fa fa-download" ></i> İndir</div>',
                                        btnClass: 'btn-green',
                                        keys: ['enter'],
                                        action: function () {

                                            window.location.href = response["linkSade"];
                                            $(".loading").show();
                                            setTimeout(function () { $(".loading").hide(); }, 2000);
                                        }
                                    }
                                }
                            });


                        } else {
                            //alert(response);
                            $(".loading").hide();
                            $.alert({
                                title: "<i class='fa fa-exclamation-triangle' style='color:red;font-size:30px'></i> Warning",
                                content: "Link is faulty or an error has occurred",
                                theme: 'modern'
                            });

                        }
                    },
                    error: function (response) {
                        $(".loading").hide();
                        $.alert({
                            title: "<i class='fa fa-exclamation-triangle' style='color:red;font-size:30px'></i> Warning",
                            content: "Something went wrong.",
                            theme: 'modern'
                        });

                    },
                    complete: function () {
                        $("#runningg").removeClass("running");
                        $(".loading").hide();
                    }
                });
            } else {
               $(".loading").hide();
                $.alert({
                    title: "Warning",
                    content: "Enter Link Before "
                });
            }
        }
        $(window).on("unload", function (e) {
          
            $(".loading").hide();
        });
  

    </script>
